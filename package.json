    "@angular/platform-browser": "^4.1.3",
    "@angular/platform-browser-dynamic": "^4.1.3",
    "@angular/router": "^4.1.3",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "log4js": "^1.1.1",
    "pug": "~2.0.0-beta11",
    "rxjs": "^5.4.0",
    "serve-favicon": "~2.4.2",
