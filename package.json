{
  "name": "express-angular-ts-starter-kit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nodemon ./dist/bin/www",
    "grunt": "grunt",
    "start": "concurrently \"webpack -w\" \"grunt watch\" \"node ./dist/bin/www\"",
    "start:client": "webpack-dev-server --inline --progress --port 8080",
    "test:client": "karma start karma.webpack.conf.js",
    "build:client": "rimraf dist/public && webpack --config config/webpack.prod.js --bail",
    "lint:client": "tslint ./client/**/*.ts -t verbose"
  },
